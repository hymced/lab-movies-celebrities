<h2>Movie details</h2>

<section class="movie-container">
	<p>
		Title: {{title}} <br />
		Genre: {{genre}} <br />
		Plot: {{plot}} <br />
		Cast: <br />
		<hr />
		{{#each cast}}
			{{this.name}} <br />
			{{this.occupation}} <br />
			{{this.catchPhrase}} <br />
			<hr />
		{{/each}}
	</p>
</section>